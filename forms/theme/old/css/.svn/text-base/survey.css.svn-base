/*	---------------------------------------------------------------------------------------
	Resetovani stylu a nastavovani vychozich stylu
	- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -	*/

html,body,div,span,h1,h2,h3,h4,h5,h6,ol,ul,li,dl,dt,dd,caption,table,tr,th,td,thead,tbody,tfoot,p,blockquote,cite,q,abbr,acronym,address,a,em,strong,img,ins,del,dfn,kbd,pre,xmp,code,form,fieldset,legend,label,object,hr{
    margin:0;padding:0;line-height:100%;
    word-wrap: break-word;
}

img{vertical-align:middle;}
a img, fieldset{border:none;}
input,textarea{font-family:Tahoma;}
hr{display:none;}
ul{list-style:none;}
table {border-collapse:collapse; table-layout: fixed;}
table tr td:first-child {width: 150px; }
td,th{text-align:left;font-weight:normal;}
abbr,acronym{border-bottom:1px dotted #000; cursor:help;}

body {font-family:Tahoma, arial, "Lucida Grande CE", lucida, sans-serif; font-size:100%;}

/* - notification - */
#notification {
    clear:both;margin:0; padding:5px 10px 8px 75px; margin:15px 0; text-align:left;
    background:#FFFFAC url(../img/warning.png); border:1px solid #F2D545;
    background-repeat: no-repeat;
    background-position: 15px 14px;
    min-height:60px;
}
#notification p {font-size:18px !important;}

/* - header - */

#head_container {clear:both; width:100%; margin:0; padding:0; text-align:center;}
#head {clear:both; margin:20px auto 0 auto; padding:20px; text-align:left;}

/* - main content - */

#content_container {clear:both; width:100%; margin:0; padding:0; text-align:center;}
#content {margin:0 auto 20px auto; text-align:left; padding:10px 20px; font-size:12px;}

#content p {line-height:1.4; padding:5px 0 8px 0;}
#content a {text-decoration:underline;}
#content a:hover {text-decoration:none;}
#content i.required {color:red; font-weight:bold; margin:0; padding:4px 5px 0px 5px; font-style:normal; display:inline-block; cursor:help;}

/* - footer - */

#footer_container {clear:both; width:100%; margin:0; padding:0; text-align:center;}
#footer {margin:0 auto; padding:0px 20px 20px 20px; text-align:left;}
#footer p {font-size:11px; line-height:14px;}
#footer p img {vertical-align:middle; padding:0;}
#footer p a:hover {text-decoration:none;}

#footer p.promo {text-align:left; padding:0 0 0 0px; margin:0; width:450px; z-index:1000;}
.promo a {line-height:65px;}
.promo a.promo-button {text-decoration:none; display:block; float:left; width:144px; height:65px; margin-right:10px; background:url('../img/footer-button.png') 0 50% no-repeat;}
.promo a.promo-button:hover {background:url('../img/footer-button-up.png') 0 50% no-repeat;}
.promo a span {visibility:hidden;}
.promo .promo_text {position:relative; top:28px;}

#footer p.remove {text-align:right; padding:0; margin:0; width:170px;}
#footer p.remove a {padding:20px 30px 0 5px; position:relative; top:0px; margin:0; color:#000; font-family:arial, tahoma, sans-serif; font-size:14px; background:url('../img/footer-remove.png') 100% 0% no-repeat;}
#footer p.remove a:hover {color:#000; background:url('../img/footer-remove-up.png') 100% 0% no-repeat;}


/* - global classes - */

.all {clear:both;}
.clear_left {clear:left;}
.clear_right {clear:right;}
.toright {float:right;}
.toleft {float:left;}
.vertical {vertical-align:middle;}
.vpravo {text-align:right;}
.centruj, .center {text-align:center;}
.odsad {padding:5px 20px 5px 10px;}

.w10 {width:10%;}
.w20 {width:20%;}
.w30 {width:30%;}
.w40 {width:40%;}
.w50 {width:50%;}
.w60 {width:60%;}
.w70 {width:70%;}
.w80 {width:80%;}
.w90 {width:90%;}
.w100 {width:100%;}

/* survey.css ------------------------------------------------------------- */

h1 {font-size:160%; line-height:1.4; padding:10px 0;}

#content {line-height:1.5;}
#content p {font-size:100%;line-height:1.5;}

.survey-text {font-size:140%;}

#content ol, #content ul {margin:5px 0; padding-left:25px;}
#content ol li, #content ul li {line-height:1.5;}

#content table {margin:5px 0;}
#content table td {padding:3px; border:1px solid #ddd;}

#content .bar {margin:10px 0 0 0;}
#content .bar #cisla {display:inline;}

#showprogress  {position:relative;}
#showprogress #cisla {position:absolute; top:20%; left:50%; text-align:center; font-size:140%; font-weight:bold; min-width:400px; _width:400px; margin-left:-200px; color:#222}
#content .bar{margin-top:0px;}


form {margin:20px 0; padding:0;}

form fieldset {margin:0; padding:5px 0 0 0; border:none;}

form h2 {padding:5px 0; font-weight:bold; font-size:115%; line-height:1.5; font-family: arial, tahoma, sans-serif;}

form .media { margin-bottom: 25px;}
form .body { margin-bottom: 25px;}

#content form ul {margin:20px 0 20px 10px; padding:0; clear:both;}
#content form ul li {margin:0 0 10px 0; padding:0; list-style-type:none;}
#content form ul li label{line-height:1.8em;}
form ul li input, form ul li label {margin:0 5px 0 0; padding:0; vertical-align:baseline; font-size:100%;}

#content form ul.rows {margin-bottom:-20px;}
#content form ul.rows li {display:inline-block; padding-right:20px; margin-bottom:20px; width:20%; vertical-align: top;}
#content form ul.rows li.text-answer {width:40%;}

form ul.thirds li {float:left; margin-right:20px; /*font-size:150%;*/ text-align:center;}
form ul.thirds li div.obrazek {margin-bottom:5px; border: 2px solid #ddd; display:table;}
form ul.thirds li div.obrazek label.img {cursor: hand; cursor: pointer; display:table-cell; vertical-align:middle; }
form ul.thirds li div img {opacity:0.5; filter: alpha(opacity=50);}
form ul.thirds li div label.img:hover img{opacity:1;filter: alpha(opacity=100);}
/*form ul.thirds li input {vertical-align:text-top; position:absolute;left: -3000px;}*/

form p.help {padding:0 5px 0 0; font-size:90%; font-style:italic; font-family: Arial, Tahoma, "Lucida Grande CE", lucida, sans-serif;}

form table {padding:0; margin:20px 0; border-collapse:collapse;}
form table tr td {border:1px solid #ddd; padding:5px;}

form .skala {border:none;}
form .skala tr td {text-align:center;}
form .skala tr td.left {text-align:left;}

form .skala tr.first_row td {font-weight:bold;}

form textarea {margin:20px 0; border:1px solid #ddd; font-size:100%; padding-top:4px; padding-left:4px;}
form select {margin:10px 0; border:1px solid #ddd; font-size:100%;}

#content p.invalid, #content p.msg_error {
    background: #FFDFDF; color:red !important; border:1px solid #FF8F8F;
    padding: 5px 0 5px 25px;
    background-image:url(../img/exclamation.png);
    background-repeat:no-repeat;
    background-position: 5px 7px;
}

#content label.invalid {display:none !important;}
#content .fill-all input.invalid {
    background-image:url(../img/exclamation.png);
    background-repeat:no-repeat;
    background-position: center right;
}

#content .sort-criteria select {margin:0px;}
#content .sort-criteria ul, #content .sort-criteria ol {
    margin-top:10px; padding-left:0;
    /*padding:5px;
    border: 2px solid #bbb;
    float:left;*/
}

#content .sort-criteria li {padding:8px 10px 10px 10px; margin-bottom:10px; border:1px solid #eee; background-color:#fff; list-style-type:none;}
#content .sort-criteria li:hover {cursor:move; border:1px solid #666;}
#content .disabled .sort-criteria li:hover {cursor:default; border:1px solid #eee;}
#content .sort-criteria li label {vertical-align: baseline; padding-left:5px;}
#content .sort-criteria li .other {top:0; margin-left:1.5em}

#content .sort-criteria li .ui-icon {display:inline-block; position:relative; top:3px;}

form .button {font-size:15px; font-family:arial, tahoma, sans-serif;}

img.ui-datepicker-trigger {vertical-align:top; padding-top:3px; padding-left:5px;}

#content textarea {max-width:99%;}
#content img {max-width:99%;}

.other {margin-left:1.5em;}

