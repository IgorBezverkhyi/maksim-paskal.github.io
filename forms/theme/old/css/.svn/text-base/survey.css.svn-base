.star-rating input {
    position:absolute; left: -3000px;
}

.star-rating .star {
    display:inline-block;
}

.star-rating label {
    display:inline-block;
    width:24px; height:24px;
    background-image: url(../img/star-sprite.png);
    background-repeat: no-repeat;
    margin:15px 5px;
}

.star-rating span.on label {
    background-position: 0 -24px !important;
}

.star-rating label span {
    display:none;
}

.star-rating .hint {
    padding:21px 5px 19px 10px; height:14px;
    display:inline-block;
    vertical-align:top;
}

.place input { width:496px; }
.place .map {width:500px; height:400px; background-color: #eee; margin-top:5px;}
