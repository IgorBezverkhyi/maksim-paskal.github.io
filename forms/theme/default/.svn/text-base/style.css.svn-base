body {
	background:#DFE5E9;
	padding:0px;
	margin:0px;
}
.viewCenter {
	margin:auto;
	width:800px
}
.content {
	margin-top: -250px;
	margin-bottom: 48px;
	padding:30px;
	font-family:Roboto,RobotoDraft,Helvetica,Arial,sans-serif;
	background:white;
}
.viewHead {
	background:#84B013;
	height:318px;
}
.question {
	padding-bottom: 5px;
}
.question h1{
	padding:10px;
	font-size:20px;
	line-height: 135%;
}
.question table td{
	padding:10px;
}

.submit {
	background:#f7f7f7;
	border: 3px solid #73AD21;
	margin:auto;
	margin-top: 20px;
}
.hidden {
	display:none;
}
.comments {
	width: 100%;
	height: 100px;
}

input[type="submit"] {
	margin: 30px;
	padding:30px;
	vertical-align: center;
}

.rsw-picker {
	cursor:pointer;
}
.rsw-picker-message ,.rsw-picker-message-number {
	border:0px none;
	background: transparent;
}
.rsw-picker {
	display: inline-block;
}
.rsw-picker-stars{
	width:110px;
}
.rsw-starred{
	background: no-repeat url(images/gimage.png) -82px -51px!important;
	display: inline-block;
    vertical-align: middle;
    width: 21px;
    height: 21px;
    float: left;
}
.rsw-unstarred {
	background: no-repeat url(images/gimage.png) -104px -114px!important;
	display: inline-block;
    vertical-align: middle;
    width: 21px;
    height: 21px;
    float: left;
}
.radio-block {
	list-style: none;
}
.radio-block li {
	padding-bottom:10px;
}
.radio-block input {
	vertical-align: top;
	margin: 0 3px 0 0;
	width: 17px;
	height: 17px;
}
.radio-block input,.radio-block label {
	cursor:pointer;
}
.radio-block label {
	margin-left:5px;
}

@media print
{
	.viewHead {
		height:0px;
	}
	.content {
		margin-top: 0px;
		margin-bottom: 0px;
		padding:0px;
	}
    .submit {
        display: none !important;
    }
}